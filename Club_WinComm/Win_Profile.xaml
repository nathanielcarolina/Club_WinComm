<Window x:Class="Club_WinComm.Win_Profile"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Club_WinComm"
        mc:Ignorable="d"
        Title="Win_Profile" Height="450" Width="800">
    <Grid>
        <Ellipse x:Name="Profile_Border" Height="200" Stroke="Black" Width="200" MouseLeftButtonUp="Profile_Border_MouseLeftButtonUp">
            <Ellipse.Fill>
                <ImageBrush ImageSource="{Binding imagePath}"/>
            </Ellipse.Fill>
        </Ellipse>

    </Grid>
</Window>
